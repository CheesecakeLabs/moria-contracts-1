{
  "name": "truffle-init-webpack",
  "version": "0.0.1",
  "description": "Frontend example using truffle v3",
  "scripts": {
    "lint": "eslint ./",
    "webpack": "export TMPDIR=/tmp; `npm bin`/webpack",
    "webpack:w": "export TMPDIR=/tmp; `npm bin`/webpack --watch",
    "webpack:dev": "export TMPDIR=/tmp; `npm bin`/webpack-dev-server --host 0.0.0.0 --hot --inline --watch-poll --port 8080",
    "build": "webpack",
    "build-api": "truffle compile && babel app -s -D -d dist --presets es2015,stage-0",
    "start": "npm run webpack:dev",
    "start-api": "node dist/javascripts/routes.js"
  },
  "author": "Douglas von Kohorn",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.2",
    "jquery": "^3.2.1",
    "truffle": "^4.0.1"
  },
  "devDependencies": {
    "ace-css": "^1.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.0.0",
    "file-loader": "^1.1.4",
    "font-awesome": "^4.7.0",
    "html-webpack-plugin": "^2.28.0",
    "ipfs-api": "^12.1.7",
    "json-loader": "^0.5.4",
    "lodash.merge": "^4.6.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.2",
    "solc": "^0.4.18",
    "style-loader": "^0.13.1",
    "truffle-contract": "^2.0.3",
    "truffle-core": "^3.4.8",
    "truffle-solidity-loader": "file:../lib/truffle-solidity-loader",
    "url-loader": "^0.5.9",
    "web3": "^0.20.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
